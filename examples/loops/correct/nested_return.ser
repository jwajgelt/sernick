fun f(): Int {
    var x : Int = 0;
    
    loop {
        var y : Int = 0;
        loop {
            y = y + 1;
            if(y == 10) {
                x = x + y;
                if(x == 100) {
                    return x;
                }
                break;
            }
        }
    }
    
    0
}
    
f(); // should equal 100