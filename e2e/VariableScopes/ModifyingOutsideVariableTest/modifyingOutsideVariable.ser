var x = read();
var y = read();

const oldX = x;
const oldY = y;

fun setXY(newX: Int, newY: Int): Unit {
    x = newX;
    y = newY;
}

fun setX(newX: Int): Unit {
    x = newX;
}

fun setY(newY: Int): Unit {
    y = newY;
}


setX(10);
write(x); // 10
write(y); // oldY

x = oldX; y = oldY;

setY(33);
write(x); // oldX
write(y); // 33

setXY(100, 200);
write(x); // 100
write(y); // 200



