fun isPrime(n: Int): Bool {
    if(n == 1){ return false; }
    var i = 2;
    var isPrime = true;
    Loop {
        if(i * i > n){ 
            return isPrime;
        } 
        var k = n/i;
        const nDivisibleByI = k*i == n;
        if(nDivisibleByI){
            return false;
        }else{
            i = i + 1;
        }
    }
}

var n = read();
write("%d\n", n);
const isPrimeN : Bool = isPrime(n);
if(isPrimeN){
    write(1);
}else{
    write(0);
}
